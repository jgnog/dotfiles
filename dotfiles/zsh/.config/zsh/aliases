alias tpl='~/tf-pipeline-library/pipeline_tf.sh'
alias dnd='killall -SIGUSR1 dunst'
alias nodnd='killall -SIGUSR2 dunst'
alias pomo='killall -SIGUSR1 dunst && sleep 25m && killall -SIGUSR2 dunst && notify-send -u critical "Pomo finish"'
