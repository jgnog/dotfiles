alias c='clear'
alias ..='cd ..' 
alias ...='cd ../../' 
alias ....='cd ../../../' 
alias mkdir='mkdir -pv'
alias ls='ls --color=auto'
alias ll='ls -lh'
alias la='ls -alh'
alias mux=tmuxinator
alias vim=nvim
alias vi=nvim
alias bat=batcat
alias avenv='source venv/bin/activate'
alias aptup='sudo apt update && sudo apt upgrade'
